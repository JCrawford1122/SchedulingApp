      <TableView fx:id="tbl_Report" layoutX="21.0" layoutY="100.0" prefHeight="200.0" prefWidth="540.0">
        <columns>
          <TableColumn fx:id="col_Customer" prefWidth="129.0" text="Customer" />
          <TableColumn fx:id="col_Consultant" prefWidth="133.0" text="Consultant" />
            <TableColumn fx:id="col_Type" prefWidth="135.79998779296875" text="Type" />
            <TableColumn fx:id="col_Time" prefWidth="142.4000244140625" text="Time (USER TIME)" />
        </columns>
      </TableView>
      <ComboBox fx:id="cbox_Location" layoutX="21.0" layoutY="35.0" onAction="#selectLocation" prefWidth="150.0" promptText="Select Location" />
      <Button fx:id="btn_MainMenu" layoutX="509.0" layoutY="330.0" mnemonicParsing="false" onAction="#btnMainMenu" text="Main Menu" />
   </children>
</AnchorPane>
